{
  "name": "your_api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:producer": "node dist/producer.js",
    "start:consumer": "node dist/consumer.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm:dev": "typeorm-ts-node-commonjs -d src/modules/database/config.ts",
    "typeorm": "typeorm-ts-node-commonjs -d dist/modules/database/config.js"
  },
  "dependencies": {
    "@bull-board/api": "6.7.1",
    "@bull-board/express": "6.7.1",
    "@bull-board/fastify": "6.7.1",
    "@bull-board/nestjs": "6.7.1",
    "@fastify/compress": "8.0.1",
    "@nestjs/axios": "4.0.0",
    "@nestjs/bullmq": "11.0.1",
    "@nestjs/cache-manager": "3.0.0",
    "@nestjs/common": "11.0.3",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.0.3",
    "@nestjs/microservices": "11.0.3",
    "@nestjs/platform-fastify": "11.0.3",
    "@nestjs/typeorm": "11.0.0",
    "axios": "1.7.9",
    "bullmq": "5.34.10",
    "cache-manager": "6.3.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "fast-glob": "3.3.3",
    "ioredis": "5.4.2",
    "lodash": "4.17.21",
    "nestjs-pino": "4.2.0",
    "pg": "8.13.1",
    "pino-pretty": "13.0.0",
    "rxjs": "7.8.1",
    "ts-mixer": "6.0.4",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.1",
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "11.0.3",
    "@types/ioredis-mock": "8.2.5",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.14",
    "@types/node": "20.17.13",
    "@types/supertest": "6.0.2",
    "jest": "29.7.0",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.8.1",
    "typescript": "5.7.3"
  },
  "jest": {
    "moduleFileExtensions": ["js", "json", "ts"],
    "setupFiles": ["<rootDir>/../test/jest.setup.ts"],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": ["**/*.(t|j)s"],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
